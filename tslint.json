{
	"extends": [
		"tslint-config-prettier",
		"tslint-plugin-prettier"
	],
	"rules": {
		"indent": [true, "tabs", 4],
		"trailing-comma": false,
		"semicolon": [true, "never"],
		"triple-equals": true,
		"no-empty": false,
		"space-before-function-paren": false,
		"quotemark": [true, "single", "avoid-escape"]
	}
}
